{
  "name": "chatslowmode-zero",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "db:up": "docker-compose --env-file .env up",
    "db:down": "docker-compose --env-file .env down --volumes",
    "db:clean": "docker volume rm -f chatslowmode-zero_pgdata && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit introspect",
    "db:migrate-push": "drizzle-kit generate && drizzle-kit push",
    "db:seed": "bun ./db/seed.ts",
    "dev:next": "next dev --turbopack",
    "dev:zero": "zero-cache-dev --schema-path ./lib/zero/config.ts",
    "dev": "pnpm run /^dev:/",
    "build:next": "next build",
    "build:generate-schema": "drizzle-zero generate",
    "build": "pnpm run /^build:/",
    "start:next": "next start",
    "start:zero": "zero-cache",
    "start": "pnpm run /^start:/",
    "lint:next": "next lint",
    "lint:ts": "tsc --noEmit",
    "lint:ox": "pnpx oxlint@latest",
    "lint": "pnpm run /^lint:/",
    "zero:generate-schema": "drizzle-zero generate"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.9.1",
    "@hookform/resolvers": "^5.1.1",
    "@neondatabase/serverless": "^1.0.1",
    "@rocicorp/zero": "^0.20.2025052100",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cookies-next": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "drizzle-zero": "^0.10.8",
    "frimousse": "^0.2.0",
    "input-otp": "^1.4.2",
    "jose": "^6.0.11",
    "lucide-react": "^0.514.0",
    "motion": "^12.17.3",
    "next": "15.3.3",
    "next-auth": "5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "pg": "^8.16.0",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-day-picker": "9.7.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.62",
    "radix-ui": "latest"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^24.0.1",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.20.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "react-is": "^19.0.0",
    "@rocicorp/zero": "^0.20.2025052100"
  }
}